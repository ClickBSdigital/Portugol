// 16. Diferença entre Dois Números
// Peça dois números e exiba a diferença absoluta entre eles.

programa {
  funcao inicio() {

    // algoritmo "DiferencaEntreDoisNumeros"

    inteiro num1, num2, diferenca

    escreva("Digite o primeiro número: ")
    leia(num1)
    escreva("Digite o segundo número: ")
    leia(num2)
    diferenca = (num1 - num2)
    escreva("A diferença absoluta é: ", diferenca)

    
  }
}
